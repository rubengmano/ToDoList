<%- include('header.ejs') -%>
<div id="heading" class="box">
  <h1><%= ListTitle %></h1>
</div>

<div class="box">

  <%newListItems.forEach((element) => {%>
    <div class="item">
      <input type="checkbox">
      <p> <%=element.name%></p>
    </div>
  <%});%>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item">
        <button type="submit" name="list" value=<%= ListTitle%>>+</button>
  </form>
</div>
<%- include('footer.ejs') -%>
